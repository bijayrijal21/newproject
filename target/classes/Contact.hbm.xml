<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="myapp.model.Contact" table="employee_contact">
        <id name="employee_id" type="string" column="employee_id">
            <generator class="assigned" />
           
        </id>
        
        <property name="employee_id" type="string" column="employee_id" not-null="true" />
        <property name="empName" type="string" column="empName" not-null="true" />
        <property name="phoneNum" type="string" column="phoneNum"  not-null="true" />
        <property name="homeNum" type="string" column="homeNum"  not-null="true" />
        <property name="email" type="string" column="email"  not-null="true" />
         <property name="offNum" type="string" column="offNum"  not-null="true" />
    </class>
   
</hibernate-mapping>
